{
  "name": "expo-diag",
  "version": "1.0.0",
  "main": "build",
  "preferGlobal": true,
  "keywords": [
    "expo",
    "ios",
    "expo-diag",
    "android"
  ],
  "description": "Diagnose environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git",
    "directory": "packages/expo-diag"
  },
  "author": "Expo <support@expo.dev>",
  "license": "MIT",
  "bin": {
    "expo-diag": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build:prod",
    "lint": "eslint .",
    "watch": "yarn run build -w",
    "test": "jest",
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf ./build/"
  },
  "devDependencies": {
    "commander": "9.0.0",
    "@expo/config": "6.0.16",
    "envinfo": "7.8.1"
  }
}
